<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
						"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.nemew.blog.dao.BoardDao">


	<resultMap id="resultGetBoard" type="com.nemew.blog.model.BoardModel">
		<result property="id" column="ID" />
		<result property="title" column="TITLE" />
		<result property="content" column="CONTENT" />
		<result property="phone" column="PHONE" />
		<result property="reg_date" column="REG_DATE" />
	</resultMap>

	<select id="BoardList" resultMap="resultGetBoard">
		<![CDATA[
			select * from board
		]]>
	</select>
	
	<select id="boardDatail" resultType="com.nemew.blog.model.BoardModel">
			select * from board where id = #{keyword}
	</select>
	
	
	

</mapper>